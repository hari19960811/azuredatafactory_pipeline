		"ADLS_LS_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLS_LS'"
		},
		"SQL_DB_LS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SQL_DB_LS'",
			"defaultValue": "Integrated Security=False;Encrypt=True;Connection Timeout=30;Data Source=missionade100dbserver.database.windows.net;Initial Catalog=missionade100db;User ID=posuser"
		},
		"Sftp_LS_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Sftp_LS'"
		},
		"smart_vehicle_LS_accountKey": {
			"type": "secureString",
